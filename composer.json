{
    "name": "contentpulse/wordpress-plugin",
    "description": "ContentPulse WordPress integration â€” auto-publish AI-generated content to your WordPress site.",
    "type": "wordpress-plugin",
    "license": "GPL-2.0-or-later",
    "keywords": ["contentpulse", "wordpress", "plugin", "content", "seo", "ai"],
    "homepage": "https://github.com/contentpulseio/contentpulse-wp",
    "authors": [
        {
            "name": "ContentPulse",
            "email": "dev@contentpulse.io"
        }
    ],
    "require": {
        "php": "^8.2",
        "contentpulse/contentpulse-php": "@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0",
        "laravel/pint": "^1.0",
        "mockery/mockery": "^1.6"
    },
    "autoload": {
        "psr-4": {
            "ContentPulse\\WordPress\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ContentPulse\\WordPress\\Tests\\": "tests/"
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "../contentpulse-php-sdk",
            "options": {
                "symlink": true
            }
        }
    ],
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
